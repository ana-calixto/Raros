<div class="container">
  <h4>Quem eu sigo</h4>
  <div class="followings">
    <% followings.each do |following| %>
    <div class="following-card">
      <div class="following-pic">
      <%= link_to page_path(following) do%>
        <%= cl_image_tag following.photo.key, height: '50px', class: 'avatar' %>
      </div>
      <div class="following-name">
        <%#= following.first_name %>
      </div>
      <% end  %>
    </div>
    <% end %>
  </div>
</div>
